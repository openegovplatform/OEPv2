<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="org.oep.core.datamgt.parameter">
	<author>TrungDK</author>
	<namespace>oep_datamgt</namespace>
    <entity name="UserParameter" local-service="true">
        <column name="userParameterId" type="long" primary="true"></column>
        <column name="userId" type="long"></column>
        <column name="groupId" type="long"></column>
        <column name="companyId" type="long"></column>
        <column name="createDate" type="Date"></column>
        <column name="modifiedDate" type="Date"></column>
        <column name="applicationName" type="String"></column>
        <column name="parameterName" type="String"></column>
        <column name="title" type="String"></column>
        <column name="parameterValue" type="String"></column>
    </entity>
    <entity name="DefaultParameter" local-service="true">
        <column name="defaultParameterId" type="long" primary="true"></column>
        <column name="companyId" type="long"></column>
        <column name="createDate" type="Date"></column>
        <column name="modifiedDate" type="Date"></column>
        <column name="applicationName" type="String"></column>
        <column name="parameterName" type="String"></column>
        <column name="title" type="String"></column>
        <column name="parameterValue" type="String"></column>
        <column name="changeable" type="int"></column>
    </entity>
</service-builder>